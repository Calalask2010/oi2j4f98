# Переменные окружения для HireHand

# База данных PostgreSQL (замените YOUR_PASSWORD на ваш пароль PostgreSQL)
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@localhost:5432/hirehand
PGHOST=localhost
PGPORT=5432
PGDATABASE=hirehand
PGUSER=postgres
PGPASSWORD=YOUR_PASSWORD

# Настройки приложения
NODE_ENV=development
PORT=5000
DEBUG=true

# Секретные ключи
SESSION_SECRET=your-session-secret-here-change-in-production
JWT_SECRET=your-jwt-secret-here-change-in-production